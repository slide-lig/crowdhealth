f = open('/media/Sumit/fileWithoutStemming')

lines = [line for line in f if line.strip()]
f.close()
lines.sort()
fileToWrite = open('/media/Sumit/sortedfileWithoutStemming','w')
for line in lines:
    fileToWrite.write(str(line)+"\n")
fileToWrite.close()
    