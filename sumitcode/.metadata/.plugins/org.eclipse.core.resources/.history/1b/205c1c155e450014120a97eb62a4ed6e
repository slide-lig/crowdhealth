			PrintWriter out = new PrintWriter(new BufferedWriter(new FileWriter("/media/Sumit/fileWithoutStemming", true)));
			while((strLine = br.readLine())!=null)
			{
				String tweetText = strLine.substring(strLine.lastIndexOf("\t"));
				out.println(tweetText);
				
				
			}
					
				
					
			
		}
		catch(Exception e){
			e.printStackTrace();
		}
