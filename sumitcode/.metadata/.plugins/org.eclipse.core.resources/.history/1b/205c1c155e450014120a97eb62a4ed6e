package TestPackage;

import java.io.*;
public class TestClass {
	public static void main(String [] args )
	{
		try{
			FileInputStream fstream = new FileInputStream("/media/Sumit/FileForWorkersandNutrition/NutritionfileforworkerwithoutURLsandRepeatTweetsremovedword");
